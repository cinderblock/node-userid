{
  "name": "userid",
  "version": "1.2.30",
  "description": "Lookup uid and gid information in node.js. Uses native POSIX bindings, not processes.",
  "license": "Public Domain",
  "author": "Jen Andre <jandre@gmail.com>",
  "contributors": [
    "Cameron Tacklind <cameron@tacklind.com>"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/cinderblock/node-userid.git"
  },
  "type": "module",
  "main": "src/userid.cjs",
  "types": "src/userid.d.ts",
  "files": [
    "src/userid.{d.ts,hh,cc}",
    "src/sources.cc",
    "src/Ids.cc",
    "src/Gid.cc",
    "src/Gids.cc",
    "src/UserName.cc",
    "src/GroupName.cc",
    "src/lib/BufferSize.{cc,hh}",
    "src/lib/Error.{cc,hh}",
    "src/lib/FunctionArg.{cc,hh}",
    "binding.gyp"
  ],
  "os": [
    "!win32"
  ],
  "engines": {
    "node": ">=8.4.0"
  },
  "scripts": {
    "test": "cross-env TS_NODE_PROJECT='./test/tsconfig.json' mocha",
    "prettier": "prettier \"**\" \".*/**\" --check",
    "spelling": "cspell \"**\" \".*/**\"",
    "lint-c": "node scripts/clang-tidy.mjs src/sources.cc",
    "lint-ts": "eslint \"**\"",
    "lint": "npm run lint-c && npm run lint-ts",
    "coverage": "nyc --reporter=lcov --reporter=text npm test",
    "install": "npm run build",
    "clean": "node-gyp clean",
    "build": "node-gyp rebuild"
  },
  "gypfile": true,
  "dependencies": {
    "bindings": "^1.5.0",
    "node-addon-api": "^4.2.0"
  },
  "devDependencies": {
    "@types/mocha": "^9.0.0",
    "@types/node": "^17.0.6",
    "@typescript-eslint/eslint-plugin": "^5.8.1",
    "@typescript-eslint/parser": "^5.8.1",
    "cross-env": "^7.0.3",
    "cspell": "^5.14.0",
    "eslint": "^8.6.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-mocha": "^10.0.3",
    "eslint-plugin-node": "^11.1.0",
    "mocha": "^9.1.3",
    "node-gyp": "^8.4.1",
    "nyc": "^15.1.0",
    "prettier": "^2.5.1",
    "should": "^13.2.3",
    "ts-node": "^10.4.0",
    "typescript": "^4.5.4"
  }
}
